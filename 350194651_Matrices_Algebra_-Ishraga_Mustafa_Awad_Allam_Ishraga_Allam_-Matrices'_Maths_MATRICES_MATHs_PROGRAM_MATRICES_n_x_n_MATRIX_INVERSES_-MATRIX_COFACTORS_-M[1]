                                    <div id="pf4" class="pf w0 h0" data-page-no="4"><div class="pc pc4 w0 h0"><div class="pdf-bg bi x0 y0 w1 h1 lazyload" data-bg="profile/Ishraga-Allam-2/publication/350194651/viewer/AS:1077994718273537@1634025377438/background/4.png" style="background-size: contain;"></div><div class="t m0 x0 h2 y1 ff1 fs0 fc0 sc0 ls1 ws1">Matrices Algebra <span class="fc1">– <span class="fc2">Ishraga Mustafa Awad Allam </span></span></div><div class="t m0 x0 h2 y2 ff1 fs0 fc1 sc0 ls1 ws1">Ishraga Allam – Matrices’ Maths.   Page <span class="ff2">4 </span></div><div class="t m0 x0 h2 y3 ff1 fs0 fc1 sc0 ls1 ws1"> </div><div class="t m0 x0 h3 y4 ff3 fs0 fc1 sc0 ls1 ws1">Ishraga Allam – Matrices’ Maths. <span class="_ _0"> </span>Page 4 </div><div class="t m0 x0 h4 y5 ff2 fs0 fc1 sc0 ls1 ws1"> </div><div class="t m0 xf h2 y38 ff1 fs0 fc4 sc0 ls1 ws1">p = p + 1; </div><div class="t m0 xe h2 y39 ff1 fs0 fc4 sc0 ls1 ws1">} </div><div class="t m0 xe h2 y3a ff1 fs0 fc9 sc0 ls1 ws0">cofact<span class="fc4 ws1">[x][y] = </span><span class="fc5">det<span class="fc4 ws1">(n - 1, co[x][y]); </span></span></div><div class="t m0 xd h2 y3b ff1 fs0 fc4 sc0 ls1 ws1">} </div><div class="t m0 xc h2 y3c ff1 fs0 fc4 sc0 ls1 ws1">} </div><div class="t m0 xc h2 y3d ff1 fs0 fc4 sc0 ls1 ws1">//free(co); </div><div class="t m0 xb h2 y3e ff1 fs0 fc4 sc0 ls1 ws1">} </div><div class="t m0 x0 h2 y3f ff1 fs0 fc4 sc0 ls1 ws1">void <span class="fc5 ws0">inverse</span>(matrix A) { </div><div class="t m0 xa h2 y40 ff1 fs0 fca sc0 ls1 ws0">cof<span class="fc4 ws1">(A); </span></div><div class="t m0 xa h2 y41 ff1 fs0 fc4 sc0 ls1 ws1">textcolor(14); </div><div class="t m0 xa h2 y42 ff1 fs0 fc4 sc0 ls1 ws1">cprintf(&quot;\n\r <span class="fc6">The Inverse of the Matrix </span>\n\r&quot;); </div><div class="t m0 xa h2 y43 ff1 fs0 fc4 sc0 ls1 ws1">fprintf(Fx, &quot;\n\r <span class="fc7">The Inverse of the Matrix </span>\n\r&quot;); </div><div class="t m0 xa h2 y44 ff1 fs0 fc4 sc0 ls1 ws1">for(i = 1; i &lt;= n; i++) { </div><div class="t m0 xb h2 y45 ff1 fs0 fc4 sc0 ls1 ws1">for(j = 1; j &lt;= n; j++) { </div><div class="t m0 xc h2 y46 ff1 fs0 fc4 sc0 ls1 ws1">inv[i][j] = (1 / <span class="fc5 ws0">det</span>(n, A)) * <span class="fc9 ws0">cofact</span>[i][j]; </div><div class="t m0 xc h2 y47 ff1 fs0 fc4 sc0 ls1 ws1">//cprintf(&quot;\n\r <span class="fc8">The Inverse of the matrix is </span>%d %d %16.8lf&quot;, i, j, inv[i][j]); </div><div class="t m0 xb h2 y48 ff1 fs0 fc4 sc0 ls1 ws1">} </div><div class="t m0 xb h2 y49 ff1 fs0 fc4 sc0 ls1 ws1">printf(&quot;\n\r&quot;); </div><div class="t m0 xa h2 y4a ff1 fs0 fc4 sc0 ls1 ws1">} </div><div class="t m0 xa h2 y4b ff1 fs0 fc5 sc0 ls1 ws0">SHOW<span class="fc4 ws1">(inv); </span></div><div class="t m0 x0 h2 y4c ff1 fs0 fc4 sc0 ls1 ws1">} </div><div class="t m0 x0 h2 y4d ff1 fs0 fc4 sc0 ls1 ws1">void <span class="fc5 ws0">factors</span>(matrix M) { </div><div class="t m0 xa h2 y4e ff1 fs0 fc4 sc0 ls1 ws1">float res; </div><div class="t m0 xa h2 y4f ff1 fs0 fc4 sc0 ls1 ws1">int f; </div><div class="t m0 xb h2 y50 ff1 fs0 fc4 sc0 ls1 ws1">for(i = 1; i &lt;= n; i++) </div><div class="t m0 xc h2 y51 ff1 fs0 fc4 sc0 ls1 ws1">for(j = 1; j &lt;= n; j++) </div><div class="t m0 xd h2 y52 ff1 fs0 fc4 sc0 ls1 ws1">prod[i][j] = 0; </div><div class="t m0 xb h2 y53 ff1 fs0 fc4 sc0 ls1 ws1">for(i = 1; i &lt;= n; i++) { </div><div class="t m0 xc h2 y54 ff1 fs0 fc4 sc0 ls1 ws1">res = 1; </div><div class="t m0 xc h2 y55 ff1 fs0 fc4 sc0 ls1 ws1">for(k = 100; k &gt;= 2; k--) { </div><div class="t m0 xd h2 y56 ff1 fs0 fc4 sc0 ls1 ws1">f = 0; </div><div class="t m0 xd h2 y57 ff1 fs0 fc4 sc0 ls1 ws1">for(j = 1; j&lt;= n, fmod(M[j][i], k) == 0; j++) </div><div class="t m0 xe h2 y58 ff1 fs0 fc4 sc0 ls1 ws1">f = f + 1; </div><div class="t m0 xd h2 y59 ff1 fs0 fc4 sc0 ls1 ws1">if(f == n) { </div><div class="t m0 xd h2 y5a ff1 fs0 fc4 sc0 ls1 ws1">M[j][i] = M[j][i] / k; </div><div class="t m0 xd h2 y5b ff1 fs0 fc4 sc0 ls1 ws1">res = res * k; </div><div class="t m0 xd h2 y5c ff1 fs0 fc4 sc0 ls1 ws1">prod[i][i] = res; </div><div class="t m0 xc h2 y5d ff1 fs0 fc4 sc0 ls1 ws1">} </div><div class="t m0 xb h2 y5e ff1 fs0 fc4 sc0 ls1 ws1">} </div><div class="t m0 xa h2 y5f ff1 fs0 fc4 sc0 ls1 ws1">} </div><div class="t m0 xa h2 y60 ff1 fs0 fc5 sc0 ls1 ws0">SHOW<span class="fc4 ws1">(prod); </span></div><div class="t m0 x0 h2 y61 ff1 fs0 fc4 sc0 ls1 ws1">} </div><div class="t m0 x0 h2 y62 ff1 fs0 fc4 sc0 ls1 ws1">int <span class="fcb ws0">main</span>() { </div><div class="t m0 xa h2 y63 ff1 fs0 fc4 sc0 ls1 ws1">double dd; </div><div class="t m0 xa h2 y64 ff1 fs0 fc4 sc0 ls1 ws1">double <span class="fc5 ws0">det</span>(int , matrix ); </div><div class="t m0 xa h2 y65 ff1 fs0 fc4 sc0 ls1 ws1">void <span class="fc5 ws0">cof</span>(matrix ); </div><div class="t m0 xa h2 y66 ff1 fs0 fc4 sc0 ls1 ws1">void <span class="fc5 ws0">add</span>(matrix , matrix ); </div><div class="t m0 xa h2 y67 ff1 fs0 fc4 sc0 ls1 ws1">void <span class="fc5 ws0">sub</span>(matrix , matrix ); </div><div class="t m0 xa h2 y68 ff1 fs0 fc4 sc0 ls1 ws1">void <span class="fc5 ws0">mult</span>(matrix , matrix ); </div><div class="t m0 xa h2 y69 ff1 fs0 fc4 sc0 ls1 ws1">void <span class="fc5 ws0">transpose</span>(matrix ); </div><div class="t m0 xa h2 y6a ff1 fs0 fc4 sc0 ls1 ws1">void <span class="fc5 ws0">inverse</span>(matrix ); </div></div><div class="pi" data-data="{&quot;ctm&quot;:[1.601307,0.000000,0.000000,1.601307,0.000000,0.000000]}"></div></div>
